


<%= form_for(@city) do |f| %>
  <div class="row">
    <div class="col-lg-2 col-xs-4 col-sm-4 labelDiv">
          <div class="label_new">
            <span class="labelText"><strong>Name</strong></span>
          </div>

    </div>
    <div class="col-lg-6 col-xs-8 col-sm-8 inputSizing">
    
       <%= f.text_field :city, autocomplete: "off",id: "cityName", placeholder: "City Name" ,class: "form-control"%>
    </div>
    <div class="col-lg-4 col-xs-0 col-sm-0">
    </div>
  </div><br />





  <div class="row">
    
    <div class="col-lg-8 col-xs-12 col-sm-12 col-md-12 labelDiv">
      <center>

      <%= f.submit "Submit", class: "btn btn-primary"%>


    <%= link_to "Back", cities_path ,class: "btn btn-default", style: "margin-left:5px;" %>
      </center>
    </div>
    <div class="col-lg-4 col-xs-0 col-sm-0 col-md-0"></div>
  </div>
<%end%>








